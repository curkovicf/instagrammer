    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/web/shell/util/src/**/*.ts", "libs/web/shell/util/src/**/*.html"]
      }
    }
  },
  "tags": ["type:web-util", "scope:web"]
}
