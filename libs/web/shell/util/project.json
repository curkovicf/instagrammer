{
  "name": "web-shell-util",
  "projectType": "library",
  "sourceRoot": "libs/web/shell/util/src",
  "prefix": "instagrammer",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/web/shell/util"],
      "options": {
        "jestConfig": "libs/web/shell/util/jest.config.js",
        "passWithNoTests": true
      }
