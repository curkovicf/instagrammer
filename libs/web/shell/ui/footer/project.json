{
  "name": "web-shell-ui-footer",
  "projectType": "library",
  "sourceRoot": "libs/web/shell/ui/footer/src",
  "prefix": "ng-inst",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/web/shell/ui/footer"],
      "options": {
        "jestConfig": "libs/web/shell/ui/footer/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/web/shell/ui/footer/src/**/*.ts", "libs/web/shell/ui/footer/src/**/*.html"]
      }
    }
  },
  "tags": ["type:web-ui", "scope:web"]
}
