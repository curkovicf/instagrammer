{
  "name": "web-shared-ui-styles",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/web/shared/ui/styles/src",
  "prefix": "instagrammer",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/web/shared/ui/styles"],
      "options": {
        "jestConfig": "libs/web/shared/ui/styles/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "libs/web/shared/ui/styles/src/**/*.ts",
          "libs/web/shared/ui/styles/src/**/*.html"
        ]
      }
    }
  },
  "tags": ["type:ui", "scope:web"]
}
