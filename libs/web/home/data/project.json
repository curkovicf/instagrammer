{
  "name": "web-home-data",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/web/home/data/src",
  "prefix": "instagrammer",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
