{
  "name": "web-module-shell",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/web/module/shell/src",
  "prefix": "ng-inst",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/web/module/shell"],
      "options": {
        "jestConfig": "libs/web/module/shell/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/web/module/shell/src/**/*.ts", "libs/web/module/shell/src/**/*.html"]
      }
    }
  },
  "tags": ["type:shell", "scope:web"]
}
