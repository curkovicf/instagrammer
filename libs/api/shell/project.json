{
  "name": "api-shell",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/api/shell/src",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/api/shell/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/api/shell"],
      "options": {
        "jestConfig": "libs/api/shell/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["type:shell", "scope:api"]
}
