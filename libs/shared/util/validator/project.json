{
  "name": "shared-util-validator",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/util/validator/src",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/shared/util/validator/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/libs/shared/util/validator"
      ],
      "options": {
        "jestConfig": "libs/shared/util/validator/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": [
    "type:util",
    "scope:shared"
  ]
}
