{
  "name": "web-instagrammer",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/web/instagrammer/src",
  "prefix": "ng-inst",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/web/instagrammer",
        "index": "apps/web/instagrammer/src/index.html",
        "main": "apps/web/instagrammer/src/main.ts",
        "polyfills": "apps/web/instagrammer/src/polyfills.ts",
        "tsConfig": "apps/web/instagrammer/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/web/instagrammer/src/favicon.ico",
          "apps/web/instagrammer/src/assets",
          {
            "glob": "**/*",
            "input": "libs/web/shared/ui/assets/src/lib",
            "output": "assets"
          }
        ],
        "styles": ["apps/web/instagrammer/src/styles.scss"],
        "stylePreprocessorOptions": {
          "includePaths": ["libs/web/shared/ui/styles/src/lib"]
        },
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/web/instagrammer/src/environments/environment.ts",
              "with": "apps/web/instagrammer/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "web-instagrammer:build:production"
        },
        "development": {
          "browserTarget": "web-instagrammer:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "web-instagrammer:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["apps/web/instagrammer/src/**/*.ts", "apps/web/instagrammer/src/**/*.html"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/web/instagrammer"],
      "options": {
        "jestConfig": "apps/web/instagrammer/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["type:app"]
}
