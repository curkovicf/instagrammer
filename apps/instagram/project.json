{
  "projectType": "application",
  "root": "apps/instagram",
  "sourceRoot": "apps/instagram/src",
  "prefix": "instagrammer",
  "i18n": {
    "locales": {
      "hr": "apps/instagram/src/locale/messages.hr.xlf",
      "en": "apps/instagram/src/locale/messages.en.xlf"
    }
  },
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/instagram",
        "index": "apps/instagram/src/index.html",
        "main": "apps/instagram/src/main.ts",
        "polyfills": "apps/instagram/src/polyfills.ts",
        "tsConfig": "apps/instagram/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/instagram/src/favicon.ico",
          "apps/instagram/src/assets"
        ],
        "styles": ["apps/instagram/src/styles.scss"],
        "scripts": [],
        "i18nMissingTranslation": "warning",
        "localize": ["hr", "en"]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/instagram/src/environments/environment.ts",
              "with": "apps/instagram/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "en": {
          "localize": ["en"]
        },
        "hr": {
          "localize": ["hr"]
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "instagram:build:production"
        },
        "development": {
          "browserTarget": "instagram:build:development"
        },
        "hr": {
          "browserTarget": "instagram:build:hr"
        },
        "en": {
          "browserTarget": "instagram:build:en"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "proxyConfig": "apps/instagram/proxy.conf.json"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "instagram:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/instagram/src/**/*.ts",
          "apps/instagram/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/instagram"],
      "options": {
        "jestConfig": "apps/instagram/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
