{
  "name": "instagrammer",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start:ng-client": "nx serve --project web-instagrammer",
    "start:playground": "nx serve --project web-playground",
    "start:api": "cross-env STAGE=dev && docker-compose up -d && STAGE=dev nx serve --project api --watch",
    "build": "nx build",
    "test": "STAGE=dev nx test",
    "i18n:extract": "ng extract-i18n --output-path apps/instagram/src/locale",
    "prepare": "husky install",
    "set-env:dev": "cross-env STAGE=dev",
    "set-env:prod": "cross-env STAGE=prod",
    "build-tags": "npx ts-node scripts/tag.ts",
    "lint": "nx run-many --target=lint --all --parallel --fix"
  },
  "private": true,
  "eslintIgnore": [
    "index.html"
  ],
  "dependencies": {
    "@angular/animations": "~15.2.9",
    "@angular/common": "~15.2.9",
    "@angular/compiler": "~15.2.9",
    "@angular/core": "~15.2.9",
    "@angular/forms": "~15.2.9",
    "@angular/localize": "~15.2.9",
    "@angular/platform-browser": "~15.2.9",
    "@angular/platform-browser-dynamic": "~15.2.9",
    "@angular/router": "~15.2.9",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/typeorm": "^9.0.1",
    "@ngneat/falso": "^6.2.0",
    "@ngrx/component-store": "~15.4.0",
    "@ngrx/effects": "~15.4.0",
    "@ngrx/store": "~15.4.0",
    "@ngrx/store-devtools": "~15.4.0",
    "@nrwl/angular": "15.2.3",
    "@types/passport-jwt": "^3.0.7",
    "angular-feather": "^6.5.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "cropperjs": "^1.5.13",
    "joi": "^17.7.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "typeorm": "^0.3.10",
    "uuid": "^9.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.2.8",
    "@angular-eslint/eslint-plugin": "~15.2.1",
    "@angular-eslint/eslint-plugin-template": "~15.2.1",
    "@angular-eslint/template-parser": "~15.2.1",
    "@angular/cli": "~15.2.8",
    "@angular/compiler-cli": "~15.2.9",
    "@angular/language-service": "~15.2.9",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@nrwl/cli": "15.2.3",
    "@nrwl/cypress": "15.2.3",
    "@nrwl/eslint-plugin-nx": "15.2.3",
    "@nrwl/jest": "15.2.3",
    "@nrwl/linter": "15.2.3",
    "@nrwl/nest": "15.2.3",
    "@nrwl/node": "15.2.4",
    "@nrwl/workspace": "15.2.3",
