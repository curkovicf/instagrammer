{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strict": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "strictBindCallApply": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@instagrammer/api-interfaces": ["libs/api-interfaces/src/index.ts"],
      "@instagrammer/api/auth/config": ["libs/api/auth/config/src/index.ts"],
      "@instagrammer/api/auth/data-access": ["libs/api/auth/data-access/src/index.ts"],
      "@instagrammer/api/auth/feature": ["libs/api/auth/feature/src/index.ts"],
      "@instagrammer/api/auth/util": ["libs/api/auth/util/src/index.ts"],
      "@instagrammer/api/core": ["libs/api/core/src/index.ts"],
      "@instagrammer/api/core/feature": ["libs/api/core/feature/src/index.ts"],
      "@instagrammer/api/shared/api-repositories": ["libs/api/shared/api-repositories/src/index.ts"],
      "@instagrammer/api/shared/app-config": ["libs/api/shared/app-config/src/index.ts"],
      "@instagrammer/api/shared/data-access/interfaces": ["libs/api/shared/data-access/interfaces/src/index.ts"],
      "@instagrammer/api/shell": ["libs/api/shell/src/index.ts"],
      "@instagrammer/web/auth/data-access": ["libs/web/auth/data-access/src/index.ts"],
      "@instagrammer/web/auth/feature": ["libs/web/auth/feature/src/index.ts"],
      "@instagrammer/web/auth/feature/login": ["libs/web/auth/feature/login/src/index.ts"],
      "@instagrammer/web/auth/feature/register": ["libs/web/auth/feature/register/src/index.ts"],
      "@instagrammer/web/auth/feature/shell": ["libs/web/auth/feature/shell/src/index.ts"],
      "@instagrammer/web/auth/ui/login": ["libs/web/auth/ui/login/src/index.ts"],
      "@instagrammer/web/auth/ui/register": ["libs/web/auth/ui/register/src/index.ts"],
      "@instagrammer/web/auth/util": ["libs/web/auth/util/src/index.ts"],
      "@instagrammer/web/shell": ["libs/web/shell/src/index.ts"],
      "@instagrammer/web/shell/feature": ["libs/web/shell/feature/src/index.ts"],
      "@instagrammer/web/shell/ui/not-found": ["libs/web/shell/ui/not-found/src/index.ts"],
      "@instagrammer/web/shell/util": ["libs/web/shell/util/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
